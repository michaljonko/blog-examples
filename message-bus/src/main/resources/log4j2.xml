<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5level] %d{HH:mm:ss.SSS} [%t] %c{1} - %m%n"/>
        </Console>
        <!--<File name="File" append="false" fileName="all.log">-->
        <!--<PatternLayout pattern="[%-5level] %d{HH:mm:ss.SSS} [%t] %c.%M - %m%n"/>-->
        <!--</File>-->
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console" level="INFO"/>
            <!--<AppenderRef ref="File" level="DEBUG"/>-->
        </Root>
    </Loggers>
</Configuration>



